  "dependencies": {
    "leaflet": "^0.7.5",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-leaflet": "^0.8.0",
    "react-mdl": "^0.15.0",
    "react-redux": "^4.0.0",
    "redux": "^3.0.4",
    "rest": "^1.3.1"
  },
  "devDependencies": {
    "babel": "^5.8.34",
    "babelify": "^6.3.0",
